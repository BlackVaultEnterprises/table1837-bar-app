# Table 1837 Bar Web App - Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Cloudinary Configuration
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret
CLOUDINARY_URL=cloudinary://api_key:api_secret@cloud_name

# AI/LLM Configuration
GEMINI_API_KEY=your_gemini_api_key
OPENROUTER_API_KEY=your_openrouter_api_key

# GitHub Configuration (for auto-commits)
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_REPO_OWNER=BlackVaultEnterprises
GITHUB_REPO_NAME=table1837-bar-app

# Vercel Configuration
VERCEL_TOKEN=your_vercel_token

# NextAuth Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_here
SENTRY_ORG=black-vault-enterprises
SENTRY_PROJECT=table1837-bar-app

# Development/Production Environment
NODE_ENV=development

